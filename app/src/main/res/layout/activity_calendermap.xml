    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#1f2936"
    android:background="#1f2936">
    tools:context=".Calendermap">

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="353dp"
